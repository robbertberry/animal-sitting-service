<tr class="bg-white border-b">
  <td class="px-6 py-4">
    <%= booking.first_name %>
  </td>

  <td class="px-6 py-4">
    <%= booking.last_name %>
  </td>

  <td class="px-6 py-4">
    <%= booking.animal_name %>
  </td>

  <td class="px-6 py-4">
    <%= booking.animal_type %>
  </td>

  <td class="px-6 py-4">
    <%= booking.hours_requested %>
  </td>

  <td class="px-6 py-4">
    <%= booking.date_of_service %>
  </td>

  <td class="px-6 py-4">
    <%= number_to_currency(booking.total_cost) %>
  </td>
</tr>